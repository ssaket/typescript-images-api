{
  "name": "typescript-stockpile-images",
  "version": "1.0.0",
  "description": "A typescript library to Search and Download images from Flickr, Pexels, Pixabay, Unsplash.",
  "main": "index.js",
  "repository": "https://github.com/ssaket/typescript-stockpile-images",
  "author": "Saket Saurabh",
  "license": "MIT",
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.15.4",
    "@types/jest": "^27.0.1",
    "jest": "^27.2.0",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "watch-ts": "tsc -w",
    "build-ts": "tsc",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --forceExit --coverage --verbose",
    "start": "node dist/Flickr.js",
    "prestart": "yarn build-ts"
  },
  "dependencies": {
    "node-fetch": "^3.0.0"
  }
}